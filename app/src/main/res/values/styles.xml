<?xml version="1.0"?>

    <resources>

    <!-- Base application theme. -->



    -<style parent="Theme.MaterialComponents.Light.DarkActionBar" name="AppTheme">

    <!-- Customize your theme here. -->


    <item name="colorPrimary">@color/colorPrimary</item>

    <item name="colorPrimaryDark">@color/colorPrimaryDark</item>

    <item name="colorAccent">@color/colorAccent</item>

</style>


    <style name="AppTheme.NoActionBar">

    <item name="windowActionBar">false</item>

    <item name="windowNoTitle">true</item>

</style>

    <style parent="ThemeOverlay.MaterialComponents.Dark.ActionBar" name="AppTheme.AppBarOverlay"/>

    <style parent="ThemeOverlay.MaterialComponents.Light" name="AppTheme.PopupOverlay"/>


    <style parent="Theme.MaterialComponents.Light.Dialog.Alert" name="CustomDialog">

    <item name="android:windowBackground">@drawable/popup_background</item>

</style>

</resources>
